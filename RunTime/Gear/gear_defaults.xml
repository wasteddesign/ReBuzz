<?xml version="1.0" encoding="utf-8"?>

<!--
  *** Do not edit this file (use gear.xml) ***

  This file is currently used for: 
  - making a machine load at startup (same as using * in index.txt)
  - blacklisting machines
  - disabling multithreading
  - setting default oversampling mode
  - setting default midi channel
  - setting default machine attributes
  
  Note that you can use * and ? wildcards in the Name attribute value.

  Possible attribute keys and values:
  LoadAtStartup       "True" or "False"
  Blacklist           "True" or "False"
  Multithreading      "True" or "False"
  OversampleFactor    "1" or "2"
  MIDIInputChannel    "-1" = none, "0" = all, "1" = 1, "2" = 2, ..., "15" = 15
  MinimumMIVersion    Minimum required MachineInterface.h version (MI_VERSION)

  The following example makes all Jeskola generators except "Jeskola Bass" use 2x oversampling by default:
 
  <Machine Name="Jeskola*" Type="Generator" OversampleFactor="2"/>
  <Machine Name="Jeskola Bass" Type="Generator" OversampleFactor="1"/>

-->

<Gear>
  <Machine Name="Modern Pattern Editor" LoadAtStartup="True" DataFormat="PatternXP"/>
  <Machine Name="Jeskola Pattern XP" LoadAtStartup="True" DataFormat="PatternXP"/>
  <Machine Name="Jeskola Pattern XP mod" LoadAtStartup="True" DataFormat="PatternXP"/>  
  <Machine Name="Modern Pianoroll" LoadAtStartup="True" DataFormat="PatternXP"/>
  <Machine Name="Polac VST 1.1" LoadAtStartup="True" MinimumMIVersion="37"/>
  <Machine Name="Polac VSTi 1.1" LoadAtStartup="True" MinimumMIVersion="37"/>
  <Machine Name="Jeskola XS-1">
    <Attribute Key="Enable MIDI" Value="1"/>    <!-- default machine attribute values can be changed like this -->
  </Machine>

  <Machine Name="CyanPhase Buzz OverLoader" Blacklist="True"/>
  <Machine Name="CyanPhase DX Instrument Adapter" Blacklist="True"/>
  <Machine Name="CyanPhase DX Effect Adapter" Blacklist="True"/>
  <Machine Name="CyanPhase DMO Effect Adapter" Blacklist="True"/>
  <Machine Name="CyanPhase AuxReturn" Blacklist="True"/>
  <Machine Name="HD AuxRepeater" Blacklist="True"/>
  <Machine Name="Jeskola AuxSend" Blacklist="True"/>
  <Machine Name="ld auxsend" Blacklist="True"/>
  <Machine Name="WhiteNoise AuxReturn" Blacklist="True"/>

  <Machine Name="Rymix*" Multithreading="False"/>
  <Machine Name="FireSledge ParamEQ" Multithreading="False"/>
  <Machine Name="BTDSys Pulsar" Multithreading="False"/>


</Gear>
